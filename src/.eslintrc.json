{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus":"off",
        "no-console":"off",
        "no-unused-vars":"off",
        "import/prefer-default-export": "off",
        "no-param-reassign":"off"
    }
}
